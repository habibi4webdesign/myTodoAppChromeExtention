{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "favicon.ico"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./static/js/content.js"],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"],
  "optional_permissions": ["<all_urls>"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
